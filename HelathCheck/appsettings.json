{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HealthChecks-UI": {
    "HealthChecks": [
      {
        "Name": "WebApp",
        "Uri": "http://localhost:5037/health"
      }
    ],

    "EvaluationTimeOnSeconds": 3,
    "MinimumSecondsBetweenFailureNotification": 60
  }
}
